{
   "env":
   {
      "name"              : "Pixelcopter-PLE-v0",
      "max_episode_steps" : 500,
      "state_type"        : "feature",
      "wrappers"          : [],
      "env_kwargs"        : {}
   }
   ,

   "agent": 
   {

      "dqn"                  : {"double" : false},
      "ddqn"                 : {"double" : true},
      "bootstrapped_ddqn"    : {"double" : true, "kernel" : null, "kernel_param" : null, "K" : 10, "mask_p" : 1},
      "our_bootstrapped_dqn" : {"double" : false, "kernel" : "gaussian_cdf", "kernel_param" : 1.0, "K" : 10, "mask_p" : 1}
   }
   ,

  "seed"             : 0,
  "timesteps"        : 5e6,
  "epoch_length"     : 5e3,
  "eval_episodes"    : 100,
  "dqn_weights"      : null,
  "input_norm"       : false,
  "input_norm_prior" : null,
  "gamma"            : 0.99,
  "eps_init"         : 1.0,
  "eps_final"        : 0.01,
  "eps_decay_steps"  : 1e3,
  "tgt_update_freq"  : 200,
  "net_struc_dqn"    : [[64, "relu"], [64, "relu"], "identity"],
  "optimizer"        : "Adam",
  "loss"             : "MSELoss",
  "lr"               : 0.0001,
  "buffer_length"    : 1e4,
  "grad_clip"        : false,
  "grad_rescale"     : false,
  "act_start_step"   : 5000,
  "upd_start_step"   : 5000,
  "upd_every"        : 1,
  "batch_size"       : 32,
  "device"           : "cpu"
}