      "scdqn_4"        : {"sc_beta" : 4.0},
      "ac_cddqn_2"     : {"AC_K" : 2},
      "ac_cddqn_3"     : {"AC_K" : 3},
      "ac_cddqn_4"     : {"AC_K" : 4},
      "boot_dqn"       : {"K" : 10, "mask_p" : 1, "grad_rescale" : true},
      "te_boot_dqn_5"  : {"kernel" : "test", "kernel_param" : 0.05, "K" : 10, "mask_p" : 1, "grad_rescale" : true},
      "te_boot_dqn_10" : {"kernel" : "test", "kernel_param" : 0.10, "K" : 10, "mask_p" : 1, "grad_rescale" : true},
      "te_boot_dqn_15" : {"kernel" : "test", "kernel_param" : 0.15, "K" : 10, "mask_p" : 1, "grad_rescale" : true},
      "te_boot_dqn_25" : {"kernel" : "test", "kernel_param" : 0.25, "K" : 10, "mask_p" : 1, "grad_rescale" : true},
      "ke_boot_dqn"    : {"kernel" : "gaussian_cdf", "kernel_param" : 1.0, "K" : 10, "mask_p" : 1, "grad_rescale" : true}
