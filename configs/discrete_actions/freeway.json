{
   "env":
   {
      "name"              : "Freeway-MinAtar-v0",
      "max_episode_steps" : -1,
      "state_type"        : "image",
      "wrappers"          : ["MinAtar_wrapper"],
      "wrapper_kwargs"    : {"MinAtar_wrapper" : {}},
      "env_kwargs"        : {}
   }
   ,

   "agent": 
   {
      "dqn"                    : {"double" : false, "self_correcting_beta" : null, "grad_rescale" : false},
      "ddqn"                   : {"double" : true, "self_correcting_beta" : null, "grad_rescale" : false},
      "scdqn_2"                : {"double" : false, "self_correcting_beta" : 2.0, "grad_rescale" : false},
      "scdqn_3"                : {"double" : false, "self_correcting_beta" : 3.0, "grad_rescale" : false},
      "scdqn_4"                : {"double" : false, "self_correcting_beta" : 4.0, "grad_rescale" : false},
      "bootstrapped_ddqn"      : {"double" : true, "kernel" : null, "kernel_param" : null, "K" : 10, "mask_p" : 1, "grad_rescale" : true},
      "our_bootstrapped_dqn_T" : {"double" : false, "kernel" : "test", "kernel_param" : 0.10, "K" : 10, "mask_p" : 1, "grad_rescale" : true},
      "our_bootstrapped_dqn_K" : {"double" : false, "kernel" : "gaussian_cdf", "kernel_param" : 1.0, "K" : 10, "mask_p" : 1, "grad_rescale" : true}
   }
   ,

  "seed"             : 0,
  "timesteps"        : 5e6,
  "epoch_length"     : 1e4,
  "eval_episodes"    : 20,
  "dqn_weights"      : null,
  "input_norm"       : false,
  "input_norm_prior" : null,
  "gamma"            : 0.99,
  "eps_init"         : 1.0,
  "eps_final"        : 0.1,
  "eps_decay_steps"  : 1e5,
  "tgt_update_freq"  : 1e3,
  "net_struc_dqn"    : null,
  "optimizer"        : "Adam",
  "loss"             : "MSELoss",
  "lr"               : 0.0001,
  "buffer_length"    : 1e5,
  "grad_clip"        : false,
  "act_start_step"   : 5000,
  "upd_start_step"   : 5000,
  "upd_every"        : 1,
  "batch_size"       : 32,
  "device"           : "cpu"
}