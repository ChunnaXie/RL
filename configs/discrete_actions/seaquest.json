  "lr"               : 0.0001,
  "buffer_length"    : 1e5,
  "grad_clip"        : false,
  "act_start_step"   : 5000,
  "upd_start_step"   : 5000,
  "upd_every"        : 1,
  "batch_size"       : 32,
  "device"           : "cpu"
}