{
   "env":
   {
      "name"              : "InvertedDoublePendulumPyBulletEnv-v0",
      "max_episode_steps" : 1000,
      "state_type"        : "feature",
      "env_kwargs"        : {},
      "wrappers"          : ["gym_POMDP_wrapper"],
      "wrapper_kwargs"    : {"gym_POMDP_wrapper" : {"env_name" : "InvertedDoublePendulumPyBulletEnv-v0", "pomdp_type" : "remove_velocity"}},
      "info"              : "RV"
   }
   ,

   "agent": 
   {
      "ddpg" : {"double_critic" : false, "tgt_pol_smooth" : false, "pol_upd_delay" : 1},
      "td3"  : {"double_critic" : true, "tgt_pol_smooth" : true, "pol_upd_delay" : 2}
   }
   ,

  "seed"             : 50,
  "timesteps"        : 5e6,
  "epoch_length"     : 5e3,
  "eval_episodes"    : 10,
  "actor_weights"    : null,
  "critic_weights"   : null,
  "input_norm"       : false,
  "input_norm_prior" : null,
  "tgt_noise"        : 0.2,
  "tgt_noise_clip"   : 0.5,
  "gamma"            : 0.99,
  "tau"              : 0.001,
  "net_struc_actor"  : [[256, "relu"], [256, "relu"], "tanh"],
  "net_struc_critic" : [[256, "relu"], [256, "relu"], "identity"],
  "lr_actor"         : 0.0001,
  "lr_critic"        : 0.0001,
  "buffer_length"    : 1e5,
  "grad_clip"        : false,
  "grad_rescale"     : false,
  "act_start_step"   : 5000,
  "upd_start_step"   : 5000,
  "upd_every"        : 1,
  "batch_size"       : 32,
  "device"           : "cpu"
}